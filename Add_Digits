class Solution {
public:
    int addDigits(int num) {
        if(num / 10 == 0){
            return num;
        }else{
            int newNum = 0;
            while(num > 0) {
                newNum += num % 10;
                num = num/10;
            }
            return addDigits(newNum);
        }
    }
};
